  "bugs": {
    "url": "https://github.com/Oxygens84/JSstore/issues"
  },
  "homepage": "https://github.com/Oxygens84/JSstore#readme",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "express": "^4.17.1",
    "moment": "^2.24.0",
    "nodemon": "^1.19.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^3.1.0",
    "file-loader": "^4.1.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "url-loader": "^2.1.0",
    "webpack": "^4.37.0",
    "webpack-cli": "^3.3.6",
    "webpack-command": "^0.4.2",
    "webpack-node-externals": "^1.7.2",
    "whatwg-fetch": "^3.0.0"
  }
}
